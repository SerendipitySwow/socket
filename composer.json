{
  "name": "swow-cloud/sockets",
  "type": "library",
  "description": "Development of nsq client based on swow",
  "keywords": [
    "swow",
    "coroutine",
    "async",
    "sockets",
    "server",
    "swoole",
    "nsq",
    "serendipity"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "codingheping",
      "email": "847050412@qq.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-sockets": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "SerendipitySwow\\Socket\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SerendipitySwowTest\\Socket\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": [ "@php vendor/bin/php-cs-fixer fix" ]
  },
  "support": {
    "pull-request": "https://github.com/swow-cloud/socket/pulls",
    "issues": "https://github.com/swow-cloud/socket/issues"
  }
}
